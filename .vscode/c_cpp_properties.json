{
    "configurations": [
        {
            "name": "CH32L103-RISC-V",
            "includePath": [
                "${workspaceFolder}/Core",
                "${workspaceFolder}/Debug",
                "${workspaceFolder}/Peripheral/inc",
                "${workspaceFolder}/User",
                "${config:WCH_TOOLCHAIN_BASE}/${config:WCH_GCC_NAME}/riscv-wch-elf/include",
                "${config:WCH_TOOLCHAIN_BASE}/${config:WCH_GCC_NAME}/lib/gcc/riscv-wch-elf/*/include",
                "${config:WCH_TOOLCHAIN_BASE}/${config:WCH_GCC_NAME}/lib/gcc/riscv-wch-elf/*/include-fixed"
            ],
            "defines": [
                "__GNUC__",
                "__riscv",
                "__riscv_xlen=32",
                "__riscv_flen=0",
                "__riscv_atomic",
                "__riscv_mul",
                "__riscv_div",
                "__riscv_compressed",
                "__riscv_cmodel_medlow",
                "__riscv_abi_ilp32",
                "HSE_VALUE=32000000",
                "USE_STDPERIPH_DRIVER"
            ],
            "compilerPath": "${config:WCH_TOOLCHAIN_BASE}/${config:WCH_GCC_NAME}/bin/riscv-wch-elf-gcc",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-march=rv32imacxw",
                "-mabi=ilp32",
                "-msmall-data-limit=8",
                "-msave-restore",
                "-fmessage-length=0",
                "-fsigned-char",
                "-ffunction-sections",
                "-fdata-sections",
                "-fno-common"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${config:WCH_TOOLCHAIN_BASE}/${config:WCH_GCC_NAME}/riscv-wch-elf/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}